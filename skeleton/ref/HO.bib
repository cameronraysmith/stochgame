keywords = {alg-coalg duality,category theory,coalgebra,cognition,language,neuroscience},
mendeley-tags = {alg-coalg duality,category theory,coalgebra,cognition,language,neuroscience},
month = apr,
number = {4},
pages = {e35028},
title = {{Categorial Compositionality III: F-(co)algebras and the Systematicity of Recursive Capacities in Human Cognition}},
volume = {7},
