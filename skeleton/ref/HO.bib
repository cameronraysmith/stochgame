issn = {1538-7445},
journal = {Cancer research},
keywords = {Animals,Breast Neoplasms,Breast Neoplasms: drug therapy,Breast Neoplasms: metabolism,Breast Neoplasms: pathology,Cathepsin B,Cathepsin B: antagonists \& inhibitors,Cathepsin B: metabolism,Cell Line,Experimental,Experimental: drug therapy,Experimental: metabolism,Experimental: pathology,Female,Humans,Hydrogen-Ion Concentration,Liver Neoplasms,Liver Neoplasms: metabolism,Liver Neoplasms: prevention \& control,Liver Neoplasms: secondary,Lung Neoplasms,Lung Neoplasms: metabolism,Lung Neoplasms: prevention \& control,Lung Neoplasms: secondary,Male,Melanoma,Mice,Neoplasm Invasiveness,Nude,Prostatic Neoplasms,Prostatic Neoplasms: drug therapy,Prostatic Neoplasms: metabolism,Prostatic Neoplasms: pathology,SCID,Sodium Bicarbonate,Sodium Bicarbonate: pharmacology,Tumor},
