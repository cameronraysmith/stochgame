keywords = {Algorithms,Animals,Behavior, Animal,Biological Evolution,Computer Simulation,Extremities,Extremities: anatomy \& histology,Extremities: innervation,Neural Networks (Computer),Robotics,Robotics: methods,evolutionary robotics},
mendeley-tags = {evolutionary robotics},
