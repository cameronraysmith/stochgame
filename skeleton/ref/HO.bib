journal = {Genetical research},
keywords = {Adaptation,Animal,Animals,Biological,Biological Evolution,Computer Simulation,Diploidy,Epistasis,Gene Frequency,Genetic,Genetics,Models,Pedigree,Phenotype,Population,Recombination,Selection,Sexual Behavior,shifting balance},
mendeley-tags = {shifting balance},
