journal = {Nature},
keywords = {Animals,Biological,Conserved Sequence,Conserved Sequence: genetics,Developmental,Developmental: genetics,Drosophila,Drosophila Proteins,Drosophila Proteins: genetics,Drosophila: classification,Drosophila: embryology,Drosophila: genetics,Evolution,Gene Expression Regulation,Genes,Genetic,Genome,Insect,Insect: genetics,Models,Molecular,Oligonucleotide Array Sequence Analysis,Phylogeny,Selection,Species Specificity,Time Factors,development,gene expression},
