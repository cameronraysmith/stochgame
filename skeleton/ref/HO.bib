journal = {PNAS},
keywords = {Bacterial,Burkholderia cepacia complex,Burkholderia cepacia complex: classification,Burkholderia cepacia complex: physiology,Ecosystem,Genome,Regression Analysis,Soil Microbiology,multiscale modelling},
mendeley-tags = {multiscale modelling},
