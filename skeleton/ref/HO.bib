issn = {1095-9203},
journal = {Science (New York, N.Y.)},
keywords = {Animal,Animal Communication,Animals,Biological Evolution,Concept Formation,Humans,Imitative Behavior,Language,Learning,Linguistics,Mathematics,Nervous System Physiological Phenomena,Speech,Vocalization,evolution,language},
