number = {212},
pages = {23--85},
publisher = {Springer},
title = {{Punctuated Equilibrium in Statistical Models of Generalized Coevolutionary Resilience : How Sudden Ecosystem Transitions Can Entrain Both Phenotype Expression and Darwinian Selection}},
year = {2008}
