keywords = {Acetates,Acetates: metabolism,Adenosine Triphosphate,Adenosine Triphosphate: metabolism,Bacterial Proteins,Bacterial Proteins: biosynthesis,Biological,Carbohydrate Metabolism,Escherichia coli,Escherichia coli: growth \& development,Models},
month = jun,
number = {2},
pages = {316--33},
pmid = {1886524},
title = {{Growth rate of Escherichia coli.}},
volume = {55},
year = {1991}
