journal = {PLoS biology},
keywords = {Adaptation, Physiological,Animals,Biological Evolution,Climate Change,Environment,Extinction, Biological,Forecasting,Genetic Variation,Humans,MPWrefs,Models, Biological,Phenotype,Selection, Genetic,fluctuating environment},
mendeley-tags = {MPWrefs,fluctuating environment},
month = jan,
