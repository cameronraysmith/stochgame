issn = {1548-7105},
journal = {Nature methods},
keywords = {Biochemistry,Biochemistry: methods,Biological,Computer Simulation,Kinetics,Models,Phosphorylation,Software Design,Stochastic Processes,rule-based modeling},
mendeley-tags = {rule-based modeling},
month = feb,
