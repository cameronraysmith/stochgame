doi = {10.1073/pnas.0605965104},
file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Fortunato, Barth\'{e}lemy/Proceedings of the National Academy of Sciences of the United States of America/Fortunato, Barth\'{e}lemy - 2007 - Resolution limit in community detection.pdf:pdf},
