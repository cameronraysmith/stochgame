@article{Sinha2004,
abstract = {We consider a random network of nonlinear maps exhibiting a wide range of local dynamics, with the links having normally distributed interaction strengths. The stability of such a system is examined in terms of the asymptotic fraction of nodes that persist in a non-zero state. Scaling results show that the probability of survival in the steady state agrees remarkably well with the May-Wigner stability criterion derived from linear stability arguments. This suggests universality of the complexity-stability relation for random networks with respect to arbitrary global dynamics of the system.},
