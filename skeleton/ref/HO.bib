file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Fuchs/Annual review of microbiology/Fuchs - 2010 - Alternative Pathways of Carbon Dioxide Fixation Insights into the Early Evolution of Life.pdf:pdf},
