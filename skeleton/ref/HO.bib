keywords = {Alleles,Animals,Biodiversity,Birds,Birds: anatomy \& histology,Birds: genetics,Competitive Behavior,Ecosystem,Genetic,Genetic Speciation,Genetic Variation,Mathematical Concepts,Models,Normal Distribution,Phenotype,Selection,complexity},
mendeley-tags = {complexity},
