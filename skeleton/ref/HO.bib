keywords = {Altruism,Animal,Animal: physiology,Animals,Behavior,Biological Evolution,Color,Ego,Genome,Lizards,Lizards: physiology,Male,Pigmentation,Pigmentation: genetics,Pigmentation: physiology,greenbeard},
