file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Yu et al/PloS one/Yu et al. - 2011 - RNA sequencing reveals differential expression of mitochondrial and oxidation reduction genes in the long-lived naked mole-rat when compared to mice.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
month = jan,
number = {11},
pages = {e26729},
pmid = {22073188},
title = {{RNA sequencing reveals differential expression of mitochondrial and oxidation reduction genes in the long-lived naked mole-rat when compared to mice.}},
volume = {6},
year = {2011}
}
