keywords = {Algorithms,Bacterial,Biological Transport,Escherichia coli,Escherichia coli: genetics,Escherichia coli: metabolism,Gene Expression Profiling,Gene Expression Regulation,Gene Regulatory Networks,Genetic,Genetic: genetics,Iron,Iron: metabolism,Oligonucleotide Array Sequence Analysis,Operon,Operon: genetics,Reproducibility of Results,Transcription,network inference},
