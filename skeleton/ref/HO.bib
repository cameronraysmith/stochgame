doi = {10.1021/ja204910n},
file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Yang et al/Journal of the American Chemical Society/Yang et al. - 2011 - Amplification, mutation, and sequencing of a six-letter synthetic genetic system.pdf:pdf},
