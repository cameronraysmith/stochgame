issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Bone Marrow Cells,Bone Marrow Cells: metabolism,Gene Expression,Macrophages,Macrophages: metabolism,Mice,self-organized criticality},
