journal = {PLoS computational biology},
keywords = {Adaptation, Biological,Adaptation, Biological: genetics,Animals,Biological Evolution,Environment,Genetics, Population,Genetics, Population: methods,Humans,Models, Genetic,Mutation,Mutation: physiology,Nerve Net,Nerve Net: physiology,Nucleic Acid Conformation,Phenotype,RNA,RNA: genetics,RNA: ultrastructure,Selection, Genetic},
