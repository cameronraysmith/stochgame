file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Lachmann, Jablonka/Journal of theoretical biology/Lachmann, Jablonka - 1996 - The inheritance of phenotypes an adaptation to fluctuating environments.pdf:pdf},
