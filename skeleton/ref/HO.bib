doi = {10.1073/pnas.2032324100},
file = {:C$\backslash$:/Users/cameron/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Spirin, Mirny - 2003 - Protein complexes and functional modules in molecular networks.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Biophysical Phenomena,Biophysics,Macromolecular Substances,Models, Biological,Proteins,Proteins: chemistry,Proteins: physiology,Proteomics},
