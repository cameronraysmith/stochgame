issn = {1476-4687},
journal = {Nature},
keywords = {Biological,Eukaryotic Cells,Eukaryotic Cells: enzymology,Eukaryotic Cells: metabolism,Kinetics,Mathematics,Models,Phosphoprotein Phosphatases,Phosphoprotein Phosphatases: metabolism,Phosphoproteins,Phosphoproteins: chemistry,Phosphoproteins: metabolism,Phosphorylation,Protein Kinases,Protein Kinases: metabolism,mathematical biology},
mendeley-tags = {mathematical biology},
month = jul,
