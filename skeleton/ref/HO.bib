journal = {PloS one},
keywords = {Animals,Cell Cycle,Cysteine-Rich Protein 61,Cysteine-Rich Protein 61: genetics,DNA Probes,Developmental,Embryo,Embryonic Development,Gene Expression Regulation,Genome,Mammalian,Mammalian: physiology,Mice,Notch,Notch: genetics,Oligonucleotide Array Sequence Analysis,Pattern Recognition,Physiological,Physiological: physiology,Receptors,Wnt Proteins,Wnt Proteins: genetics,network inference},
mendeley-tags = {network inference},
