journal = {PLoS computational biology},
keywords = {Artificial Intelligence,Computational Biology,Computational Biology: methods,Gene Expression Profiling,Gene Expression Profiling: methods,NMF,Oligonucleotide Array Sequence Analysis,Oligonucleotide Array Sequence Analysis: methods,Pattern Recognition, Automated,Pattern Recognition, Automated: methods,Systems Integration,nonnegative matrix factorization},
