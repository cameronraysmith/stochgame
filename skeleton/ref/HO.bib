journal = {Molecular bioSystems},
keywords = {Biological,Computational Biology,Computational Biology: methods,Gene Regulatory Networks,Metabolic Networks and Pathways,Models,Semantics,Signal Transduction,networks,power laws},
mendeley-tags = {networks,power laws},
