journal = {PLoS computational biology},
keywords = {Animal,Animals,Brain,Computer Simulation,Haruna,Models,Neurological,Vocalization,hierarchy,neuroscience},
mendeley-tags = {Haruna,hierarchy,neuroscience},
month = nov,
