journal = {PLoS genetics},
keywords = {Animals,Cohort Studies,Colonic Neoplasms,Colonic Neoplasms: genetics,Colonic Neoplasms: pathology,Computational Biology,Computational Biology: methods,Databases, Factual,Gene Expression Profiling,Gene Expression Regulation, Neoplastic,Humans,Mice,Mice, Nude,Neoplasm Metastasis,Neoplasm Metastasis: genetics,Neoplasm Metastasis: physiopathology,Neoplasms,Neoplasms: genetics,Neoplasms: metabolism,Oligonucleotide Array Sequence Analysis,RNA, Small Interfering,Reproducibility of Results,Sensitivity and Specificity,Survival,Transcription, Genetic,Xenograft Model Antitumor Assays,jessica mar,microarray analysis},
mendeley-tags = {jessica mar,microarray analysis},
