journal = {PNAS},
keywords = {Biological,Catalysis,Chemical,Chemistry,Computer Simulation,Evolution,Models,Physical,Physicochemical Phenomena,Solutions,Solutions: chemistry,Stochastic Processes,Substrate Specificity,Thermodynamics,Time Factors},
month = jun,
