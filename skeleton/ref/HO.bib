keywords = {Algorithms,Amino Acid Sequence,Bacterial Proteins,Bacterial Proteins: chemistry,Biophysical Phenomena,Biophysics,Hydrogen Bonding,Models, Molecular,Molecular Sequence Data,Peptide Fragments,Peptide Fragments: chemistry,Protein Folding,Protein Structure, Secondary,Proteins,Proteins: chemistry,TPS,Thermodynamics,transition path sampling},
