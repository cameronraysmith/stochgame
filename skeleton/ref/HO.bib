keywords = {Animals,Evolution, Molecular,Humans,Mammals,Mammals: genetics,Mammals: physiology,Mutation,Mutation: genetics,Polymorphism, Genetic,RNA,RNA, Transfer,RNA, Transfer: genetics,RNA: genetics,shifting balance},
mendeley-tags = {shifting balance},
month = mar,
number = {7286},
pages = {279--82},
pmid = {20182427},
publisher = {Nature Publishing Group},
title = {{Compensatory evolution in mitochondrial tRNAs navigates valleys of low fitness.}},
