doi = {10.1126/science.1182376},
file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Peisajovich et al/Science/Peisajovich et al. - 2010 - Rapid diversification of cell signaling phenotypes by modular domain recombination.pdf:pdf},
