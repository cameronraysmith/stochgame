keywords = {Adaptation,Computer Simulation,Evolution,Gene Regulatory Networks,Genetic,Genotype,Heredity,Humans,Male,Models,Molecular,Mutation,Physiological,Physiological: genetics,Reproducibility of Results,Selection,Stochastic Processes,Transcription,networks},
mendeley-tags = {networks},
