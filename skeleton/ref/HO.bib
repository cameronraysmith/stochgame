journal = {Cold Spring Harbor symposia on quantitative biology},
keywords = {Bacterial,Bacterial: genetics,DNA,Databases,Escherichia coli,Escherichia coli: genetics,Escherichia coli: physiology,Evolution,Genetic,Genetic Variation,Genome,Models,Molecular,Mutation,Nucleic Acid,Phenotype,Polymorphism,Single Nucleotide,Time Factors,experimental evolution},
mendeley-tags = {experimental evolution},
