@article{Mehta2009,
abstract = {Bacteria communicate using secreted chemical signaling molecules called autoinducers in a process known as quorum sensing. The quorum-sensing network of the marine bacterium Vibrio harveyi uses three autoinducers, each known to encode distinct ecological information. Yet how cells integrate and interpret the information contained within these three autoinducer signals remains a mystery. Here, we develop a new framework for analyzing signal integration on the basis of information theory and use it to analyze quorum sensing in V. harveyi. We quantify how much the cells can learn about individual autoinducers and explain the experimentally observed input-output relation of the V. harveyi quorum-sensing circuit. Our results suggest that the need to limit interference between input signals places strong constraints on the architecture of bacterial signal-integration networks, and that bacteria probably have evolved active strategies for minimizing this interference. Here, we analyze two such strategies: manipulation of autoinducer production and feedback on receptor number ratios.},
author = {Mehta, Pankaj and Goyal, Sidhartha and Long, Tao and Bassler, Bonnie L and Wingreen, Ned S},
doi = {10.1038/msb.2009.79},
file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Mehta et al/Molecular systems biology/Mehta et al. - 2009 - Information processing and signal integration in bacterial quorum sensing.pdf:pdf},
issn = {1744-4292},
journal = {Molecular systems biology},
keywords = {Feedback, Physiological,Information Theory,Protein Kinases,Protein Kinases: metabolism,Quorum Sensing,Receptors, Cell Surface,Receptors, Cell Surface: metabolism,Signal Transduction,Vibrio,Vibrio: enzymology,Vibrio: metabolism},
month = jan,
