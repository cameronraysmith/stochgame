keywords = {Anticodon,Anticodon: chemistry,Anticodon: genetics,Chemistry, Physical,Codon,Codon: chemistry,Codon: genetics,Genetic Code,Globins,Globins: genetics,HIV Protease,HIV Protease: genetics,Humans,Hydrogen Bonding,Mathematics,Models, Genetic,Mutation,Physicochemical Phenomena,Recombination, Genetic,Thermodynamics},
