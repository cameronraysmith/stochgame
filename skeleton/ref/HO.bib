journal = {Bio Systems},
keywords = {Algorithms,Biological,Citric Acid Cycle,Computational Biology,Computational Biology: methods,Computer Simulation,Gene Regulatory Networks,Krohn-Rhodes,Metabolic Networks and Pathways,Models,semigroup theory},
mendeley-tags = {Krohn-Rhodes,semigroup theory},
