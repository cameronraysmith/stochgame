journal = {Nature},
keywords = {Adaptation,Bacterial,Bacterial: genetics,DNA Mutational Analysis,Escherichia coli,Escherichia coli: genetics,Escherichia coli: growth \& development,Evolution,Genetic,Genetic Fitness,Genome,Models,Molecular,Mutation,Physiological,Selection,Time Factors,experimental evolution},
