author = {Qian, Lulu and Winfree, Erik and Bruck, Jehoshua},
doi = {10.1038/nature10262},
file = {:C$\backslash$:/Users/cameron/Documents/Mendeley/Qian, Winfree, Bruck/Nature/Qian, Winfree, Bruck - 2011 - Neural network computation with DNA strand displacement cascades.pdf:pdf},
issn = {0028-0836},
